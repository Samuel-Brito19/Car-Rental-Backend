{
  "version": 2,
  "builds": [
      {
          "src": "dist/app.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["dist/**"] }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/app.js",
          "headers": {
            "Access-Control-Allow-Origin" : "*",
            "Access-Control-Allow-Credentials": "true"

          },
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
  ]
}